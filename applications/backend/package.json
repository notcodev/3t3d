{
  "name": "@tic-tac-toe-3d/backend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "module": "dist/app/src/server.js",
  "types": "dist/types/src/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "dev": "NODE_ENV=development nodemon src/server.ts",
    "build": "shx rm -rf dist/app && tsc --noEmit && swc src -d dist/app",
    "watch:trpc": "nodemon -w src -w drizzle --exec pnpm build:types",
    "build:types": "shx rm -rf dist/types && tsc -p tsconfig.types.json && tsc-alias"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.1",
    "@fastify/websocket": "^11.0.1",
    "@tic-tac-toe-3d/core": "workspace:*",
    "@trpc/server": "^10.45.2",
    "bcrypt": "^5.1.1",
    "drizzle-orm": "^0.34.1",
    "effector": "^23.2.3",
    "envalid": "^8.0.0",
    "fastify": "^5.0.0",
    "fastify-plugin": "^5.0.1",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "patronum": "^2.2.0",
    "pg": "^8.13.0",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@swc-node/register": "1.10.3",
    "@swc/cli": "0.4.1-nightly.20240914",
    "@swc/core": "^1.7.35",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^10.0.0",
    "dotenv": "^16.4.5",
    "nodemon": "^3.1.7",
    "pino-pretty": "^11.2.2",
    "shx": "^0.3.4",
    "tsc-alias": "^1.8.10"
  }
}
